{
    "name": "marbles/craft-smoke-signal",
    "description": "Smoke Signal provides a quick way to inform your visitors.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "smoke signal",
        "warnings",
        "messages",
        "popup"
    ],
    "support": {
        "docs": "https://raw.githubusercontent.com/Marbles/craft-smoke-signal/master/README.md",
        "issues": "https://raw.githubusercontent.com/Marbles/craft-smoke-signal/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marbles",
            "homepage": "https://www.marbles.be/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "sebastianlenz/linkfield": "^1.0.20"
    },
    "autoload": {
        "psr-4": {
          "marbles\\smokesignal\\": "src/"
        }
    },
    "extra": {
        "name": "Smoke Signal",
        "handle": "smoke-signal",
        "hasCpSettings": false,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/Marbles/craft-smoke-signal/master/CHANGELOG.md",
        "components": {
            "signalsService": "marbles\\smokesignal\\services\\SignalsService"
        },
        "class": "marbles\\smokesignal\\SmokeSignal"
    }
}
